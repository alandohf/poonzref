# LEA 0x4(%esp)， %ecx
#  该指令的作用是，取出esp寄存器里的值，加上4，不再继续寻址，而是将得到值直接传递给ecx；如果是其他指令，则还需进行间接寻址，再传值。
#    http://blog.sina.com.cn/s/blog_4d1bbec70100du0f.html  

.file	"t00077.c"
	.def	___main;	.scl	2;	.type	32;	.endef
	.text
.globl _main
	.def	_main;	.scl	2;	.type	32;	.endef
_main:
	pushl	%ebp
	movl	%esp, %ebp
	subl	$8, %esp
	andl	$-16, %esp
	movl	$0, %eax
	addl	$15, %eax
	addl	$15, %eax
	shrl	$4, %eax
	sall	$4, %eax
	movl	%eax, -8(%ebp)
	movl	-8(%ebp), %eax
	call	__alloca
	call	___main
	movl	$0, -4(%ebp)
L2:
	cmpl	$254, -4(%ebp)
	jg	L3
	leal	-4(%ebp), %eax
	incl	(%eax)
	jmp	L2
L3:
	movl	$0, %eax
	leave
	ret
